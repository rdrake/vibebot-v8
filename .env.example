# .env.example
# Copy to .env and fill in your API keys
# NOTE: These env vars are optional - API keys can be configured via bot.conf instead

# For Gemini models (free tier)
GEMINI_API_KEY=your-free-gemini-key-here

# For Vertex AI models (paid tier)
VERTEX_AI_API_KEY=your-paid-vertex-key-here
VERTEX_PROJECT=your-gcp-project-id
VERTEX_LOCATION=us-central1

# For OpenAI models (optional)
OPENAI_API_KEY=your-openai-key-here

# For Anthropic models (optional)
ANTHROPIC_API_KEY=your-anthropic-key-here
